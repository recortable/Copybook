<%= simple_form_for publisher do |form| %>
    <%= form.input :slug %>
    <%= form.input :name %>
    <%= form.input :email %>
    <%= form.input :banner_art%>
    <%= form.input :background_art%>
    <%= form.input :description %>
    <%= form.input :color_body %>
    <%= form.input :color_text %>
    <%= form.input :color_links %>
    <%= form.input :color_background %>
    <%= form.submit %>
<% end %>